{
  "name": "quickstart",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "lint": "biome check --error-on-warnings --apply .",
    "prebuild": "bun run lint",
    "build": "bun build --compile --minify --sourcemap src/index.ts --outfile runapp",
    "prestart": "bun run lint",
    "start": "bun src/index.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.2",
    "@eslint/js": "^9.1.1",
    "@types/bun": "latest",
    "@types/figlet": "^1.5.8",
    "eslint": "^9.1.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "figlet": "^1.7.0"
  }
}
