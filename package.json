{
  "name": "quickstart",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "test": "bun test",
    "clean": "rimraf dist build out",
    "start": "NODE_ENV=production bun src/index.ts",
    "build": "bun build src/index.ts --outdir out",
    "dev": "bun build src/index.ts --outdir out --watch",
    "lint": "biome check --error-on-warnings --apply-unsafe .",
    "prelint": "bun run clean",
    "predev": "bun run lint",
    "pretest": "bun run lint",
    "prebuild": "bun run lint",
    "prestart": "bun run lint"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.2",
    "@eslint/js": "^9.1.1",
    "@types/bun": "latest",
    "@types/figlet": "^1.5.8",
    "eslint": "^9.1.1",
    "rimraf": "^5.0.5"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "figlet": "^1.7.0"
  },
  "trustedDependencies": [
    "@biomejs/biome"
  ]
}
